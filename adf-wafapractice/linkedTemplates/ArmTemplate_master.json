{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-wafapractice"
		},
		"ls_stwafastudies_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_stwafastudies'"
		},
		"custeventtrig_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/ee277ae4-2486-4022-85a0-bdd7c0e127cf/resourceGroups/rg-wafastudies-adf/providers/Microsoft.Storage/storageAccounts/stwafastudiestest"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-wafapractice_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_stwafastudies_connectionString": {
						"value": "[parameters('ls_stwafastudies_connectionString')]"
					},
					"custeventtrig_properties_typeProperties_scope": {
						"value": "[parameters('custeventtrig_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}